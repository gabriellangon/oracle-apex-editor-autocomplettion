  "snippets": [
    {
      "label": "FOR..LOOP (cursor)",
      "insertText": "FOR rec IN (SELECT ${1:columns} FROM ${2:table}) LOOP\n  ${3:-- process}\nEND LOOP;",
      "detail": "Cursor FOR loop"
    },
    {
      "label": "FOR..LOOP (numeric)",
      "insertText": "FOR i IN ${1:1}..${2:10} LOOP\n  ${3:-- process}\nEND LOOP;",
      "detail": "Numeric FOR loop"
    },
    {
      "label": "IF..THEN..END IF",
      "insertText": "IF ${1:condition} THEN\n  ${2:-- action}\nEND IF;",
      "detail": "Simple IF block"
    },
    {
      "label": "IF..THEN..ELSE..END IF",
      "insertText": "IF ${1:condition} THEN\n  ${2:-- action}\nELSE\n  ${3:-- alternative}\nEND IF;",
      "detail": "IF/ELSE block"
    },
    {
      "label": "IF..THEN..ELSIF..END IF",
      "insertText": "IF ${1:condition1} THEN\n  ${2:-- action1}\nELSIF ${3:condition2} THEN\n  ${4:-- action2}\nELSE\n  ${5:-- default}\nEND IF;",
      "detail": "IF/ELSIF/ELSE block"
    },
    {
      "label": "BEGIN..EXCEPTION..END",
      "insertText": "BEGIN\n  ${1:-- code}\nEXCEPTION\n  WHEN OTHERS THEN\n    ${2:-- handle error}\nEND;",
      "detail": "Anonymous block with exception"
    },
    {
      "label": "DECLARE..BEGIN..END",
      "insertText": "DECLARE\n  ${1:-- variables}\nBEGIN\n  ${2:-- code}\nEND;",
      "detail": "Full anonymous block"
    },
    {
      "label": "DECLARE..BEGIN..EXCEPTION..END",
      "insertText": "DECLARE\n  ${1:-- variables}\nBEGIN\n  ${2:-- code}\nEXCEPTION\n  WHEN OTHERS THEN\n    ${3:-- handle error}\nEND;",
      "detail": "Full anonymous block with exception"
    },
    {
      "label": "CURSOR declaration",
      "insertText": "CURSOR ${1:cur_name} IS\n  SELECT ${2:columns}\n  FROM   ${3:table}\n  WHERE  ${4:condition};",
      "detail": "Explicit cursor declaration"
    },
    {
      "label": "CURSOR open/fetch/close",
      "insertText": "OPEN ${1:cur_name};\nLOOP\n  FETCH ${1:cur_name} INTO ${2:rec};\n  EXIT WHEN ${1:cur_name}%NOTFOUND;\n  ${3:-- process}\nEND LOOP;\nCLOSE ${1:cur_name};",
      "detail": "Full cursor processing"
    },
    {
      "label": "BULK COLLECT",
      "insertText": "SELECT ${1:columns}\nBULK COLLECT INTO ${2:collection}\nFROM ${3:table};",
      "detail": "Bulk collect into collection"
    },
    {
      "label": "FORALL..INSERT",
      "insertText": "FORALL i IN 1..${1:collection}.COUNT\n  INSERT INTO ${2:table} VALUES ${1:collection}(i);",
      "detail": "Bulk insert with FORALL"
    },
    {
      "label": "WHILE..LOOP",
      "insertText": "WHILE ${1:condition} LOOP\n  ${2:-- process}\nEND LOOP;",
      "detail": "While loop"
    },
    {
      "label": "CASE expression",
      "insertText": "CASE\n  WHEN ${1:condition} THEN ${2:result}\n  ELSE ${3:default}\nEND",
      "detail": "CASE expression"
    },
    {
      "label": "EXECUTE IMMEDIATE",
      "insertText": "EXECUTE IMMEDIATE '${1:sql_statement}' USING ${2:bind_var};",
      "detail": "Dynamic SQL execution"
    },
    {
      "label": "RAISE_APPLICATION_ERROR",
      "insertText": "RAISE_APPLICATION_ERROR(${1:-20001}, '${2:Error message}');",
      "detail": "Raise custom application error"
    },
    {
      "label": "PRAGMA EXCEPTION_INIT",
      "insertText": "PRAGMA EXCEPTION_INIT(${1:exception_name}, ${2:-20001});",
      "detail": "Associate exception with error code"
    },
    {
      "label": "CREATE OR REPLACE PROCEDURE",
      "insertText": "CREATE OR REPLACE PROCEDURE ${1:proc_name} (\n  ${2:p_param1} IN ${3:VARCHAR2}\n) AS\nBEGIN\n  ${4:-- code}\nEND ${1:proc_name};",
      "detail": "Create stored procedure"
    },
    {
      "label": "CREATE OR REPLACE FUNCTION",
      "insertText": "CREATE OR REPLACE FUNCTION ${1:func_name} (\n  ${2:p_param1} IN ${3:VARCHAR2}\n) RETURN ${4:VARCHAR2} AS\n  ${5:l_result} ${4:VARCHAR2};\nBEGIN\n  ${6:-- code}\n  RETURN ${5:l_result};\nEND ${1:func_name};",
      "detail": "Create stored function"
    },
    {
      "label": "TYPE..TABLE OF",
      "insertText": "TYPE ${1:t_name} IS TABLE OF ${2:VARCHAR2(100)} INDEX BY PLS_INTEGER;",
      "detail": "Associative array type"
    },
    {
      "label": "TYPE..RECORD",
      "insertText": "TYPE ${1:r_name} IS RECORD (\n  ${2:field1} ${3:VARCHAR2(100)},\n  ${4:field2} ${5:NUMBER}\n);",
      "detail": "Record type"
    }
  ]
